sqlRoleTablePrincipalName=name
sqlRoleTableIdentityGuid=user_id
sqlRoleTableSelectByIdentity=SELECT * FROM groups, user_groups WHERE groups.id = user_groups.group_id AND user_groups.user_id = ${roleIdentityGuid}


